{
  "name": "trixly",
  "version": "0.1.4",
  "description": "Universal TypeScript starter kit for Angular 2 and React",
  "main": "src/index.ts",
  "directories": {
    "src": "src/"
  },
  "files": [
    "CHANGELOG.md",
    "CLA.md",
    "CONTRIBUTING.md",
    "LICENSE.md",
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf coverage/ dist/",
    "clean:dist": "rimraf -- dist/",
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack/webpack.dev.config.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.prod.config.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "test": "npm run test:browser && npm run test:node",
    "test:browser": "karma start config/karma.conf.js --no-auto-watch --browsers=Chrome",
    "test:phantom": "karma start config/karma.conf.js --single-run --no-auto-watch --browsers=PhantomJS",
    "test:chrome": "karma start config/karma.conf.js --single-run --no-auto-watch --browsers=Chrome",
    "test:node": "mocha-webpack --webpack-config ./config/webpack/webpack.node.config.js test/node-tests/**/*.ts --colors",
    "watch:node": "mocha-webpack --webpack-config ./config/webpack/webpack.node.config.js test/node-tests/**/*.ts --watch --colors",
    "watch": "npm run watch:chrome",
    "watch:phantom": "karma start config/karma.conf.js --auto-watch --no-single-run --browsers=PhantomJS",
    "watch:chrome": "karma start config/karma.conf.js --auto-watch --no-single-run --browsers=Chrome",
    "watch:dev": "npm run build:dev -- --w",
    "watch:prod": "npm run build:prod -- --w",
    "lint": "tslint \"!(node_modules|typings)/**/*.ts?(x)\"",
    "lint:tests": "tslint \"test/**/*.ts?(x)\"",
    "lint:src": "tslint \"src/**/*.ts?(x)\"",
    "browser": "webpack-dev-server --quiet --config ./config/webpack/webpack.browser.config.js",
    "postversion": "git push && git push --tags",
    "dependencies:check": "ncu",
    "dependencies:upgrade": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trixler/trixly.git"
  },
  "author": {
    "name": "KFlash",
    "email": "kflash123@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trixler/trixly/issues"
  },
  "keywords": [
    "vscode",
    "typescript",
    "boilerplate",
    "project",
    "skeleton",
    "debug",
    "intellisense",
    "angular2",
    "angular",
    "react",
    "reactjs",
    "workflow",
    "microsoft",
    "universal",
    "isomorphic",
    "babel",
    "webpack",
    "karma",
    "compile",
    "transpile",
    "mocha",
    "coverage",
    "async",
    "await",
    "mocha",
    "chai"
  ],
  "homepage": "https://github.com/trixler/trixly#readme",
